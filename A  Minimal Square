import java.util.*;
public class Main{
	public static void main(String[] args) {
		Scanner sc= new Scanner (System.in);
		
		int t=sc.nextInt();
		while(t-->0){	
			int x=sc.nextInt();
			int y=sc.nextInt();
			int min=Math.min(x,y);
			int max=Math.max(x,y);
			if(2*min>=max){
				System.out.println(4*min*min);
			}else{
				System.out.println(max*max);
			}
			
		}
	}
	
}
