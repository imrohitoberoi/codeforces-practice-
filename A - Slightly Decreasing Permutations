import java.util.*;
public class Main {
    
 public static void main(String[] args) {
		 Scanner s = new Scanner(System.in);
	     int n = s.nextInt();
         int k = s.nextInt();
         for(int i=n;i>=n-k+1;i--){
             System.out.print(i+" ");
         }
         for(int i=1;i<=n-k;i++){
             System.out.print(i+" ");
         }
	    }

        

	}
