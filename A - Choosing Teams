import java.util.*;
public class MyClass {
    public static void main(String args[]) {
   Scanner sc = new Scanner(System.in);
   int n = sc.nextInt();
   int k = sc.nextInt();
   int[]  arr = new int[n];
   for(int i =0;i<n;i++){
       arr[i] = sc.nextInt();
   }
   
   int count =0;
   for(int i =0;i<n;i++){
       if(5-arr[i]>=k){
           count++;
       }
   }
   int res = count/3;
   System.out.println(res);
   
}}
