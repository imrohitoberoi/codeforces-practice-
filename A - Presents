import java.util.*;

public class Main
{
	public static void main(String[] args) 
	{
		Scanner s = new Scanner(System.in);
		int n= s.nextInt();
		int i,temp=0;
		int arr[]=new int[n+1];
		for(i=1;i<n+1;i++)
		{
		    arr[s.nextInt()] = i;
		   // temp = s.nextInt();
		   // arr[temp]=i;
		   // System.out.println(arr[temp]);
		}
		
		for(i=1;i<n+1;i++)
		    System.out.print(arr[i]+" ");
	}
}
