import java.util.*;
public class MyClass {
    public static void main(String args[]) {
     
        Scanner sc= new Scanner(System.in);
        int t = sc.nextInt();
        while (t-- > 0) {
            int a = sc.nextInt();
            int b = sc.nextInt();
            int n = sc.nextInt();
            int s = sc.nextInt();
         if(s/n <=a)
              s=s%n;
         else
              s-=a*n;
            System.out.println(s <= b ? "YES" : "NO");
        }
    }
}
