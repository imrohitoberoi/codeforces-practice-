import java.util.Scanner;

public class shortsubstring {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        while(t-->0){
            String s = sc.next();
            String p= s.charAt(0)+"";
            for(int i=1;i<s.length();i+=2){
p+=s.charAt(i);
            }
System.out.println(p);
        }

   sc.close();
    }
}
