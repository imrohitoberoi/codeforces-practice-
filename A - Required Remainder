A - Required Remainder

//package cfr653;
import java.util.*;
 public class Main{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        while(t-->0){

        
        
        int x = sc.nextInt();
        int y = sc.nextInt();
        int n = sc.nextInt();

        int k=n%x-y;
    if(k<0)
    k=x+k; 
	
	System.out.println(n-k);

    }
}}
